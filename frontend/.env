# Production Google Client ID (uncomment and use your real credentials)
# REACT_APP_GOOGLE_CLIENT_ID=YOUR_REAL_CLIENT_ID_HERE.apps.googleusercontent.com
# VITE_GOOGLE_CLIENT_ID=YOUR_REAL_CLIENT_ID_HERE.apps.googleusercontent.com

# Environment
REACT_APP_ENV=development