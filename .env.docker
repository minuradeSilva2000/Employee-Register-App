# Docker Environment Variables
# Copy this file to .env and update with your actual values

# JWT Secrets (Generate strong random strings for production)
JWT_SECRET=your_jwt_secret_key_here_make_it_long_and_random_for_production
JWT_REFRESH_SECRET=your_jwt_refresh_secret_key_here_make_it_long_and_random_for_production

# Session Secret (Generate strong random string for production)
SESSION_SECRET=your_session_secret_key_here_make_it_long_and_random_for_production

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# MongoDB Configuration (Optional - defaults are set in docker-compose.yml)
# MONGO_INITDB_ROOT_USERNAME=admin
# MONGO_INITDB_ROOT_PASSWORD=password123

# Note: For production, use strong passwords and secrets
# You can generate secure secrets using: openssl rand -base64 32